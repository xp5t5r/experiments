<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" version="2.5">
  <filter>
    <filter-name>echofilter</filter-name>
    <filter-class>xp5t5r.EchoFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>echofilter</filter-name>
    <url-pattern>*</url-pattern>
  </filter-mapping>
  <filter>
    <filter-name>TextServiceRestApi</filter-name>
    <filter-class>xp5t5r.TextServiceRestApi</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>TextServiceRestApi</filter-name>
    <url-pattern>*</url-pattern>
  </filter-mapping>
  <session-config>
    <session-timeout>30</session-timeout>
  </session-config>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
</web-app>
